{
  "webservice": {
    "type": "fdsnws",
    "interface": "event",
    "majorversion": "1",
    "application": "query",
    "parameters": {
      "format": "xml",
      "orderby": "time",
      "minmag": "0.1",
      "starttime": "2019-06-07T00:00:00"
    }
  },
  "repositories": {
    "IRIS": "http://service.iris.edu/fdsnws/",
    "INGV": "http://webservices.ingv.it/fdsnws/",
    "IMO": "http://www.orfeus-eu.org/fdsnws/"
  },
  "listener": {
    "interval": 30,
    "timeout": "360",
    "data_ext": ".response",
    "results_name": "events.json",
    "trigger": "cat %s",
    "deadline": 60000,
    "outputdir": "./tmp",
    "timethreshold": "5",
    "latthreshold": "0.5",
    "longthreshold": "0.5"
  }
}
