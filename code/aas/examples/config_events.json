{
  "webservice": {
    "type": "fdsnws",
    "interface": "event",
    "majorversion": "1",
    "application": "query",
    "parameters": {
      "format": "xml",
      "orderby": "time",
      "minmag": "0.1",
      "starttime": "2019-05-27T00:00:00"
    }
  },
  "repositories": [
    {
      "name": "IRIS",
      "url": "http://service.iris.edu/fdsnws/" 
    }
  ],
  "listener": {
    "interval": 20,
    "timeout": "360",
    "data_ext": ".response",
    "results_name": "events.json",
    "trigger" : "cat %s",
    "deadline" : 1800,
    "outputdir" : "./tmp"
  }
}
